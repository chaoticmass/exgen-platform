cd /root/projects/c/
cd exgen/

#!/bin/bash
gcc -w -rdynamic -g -o obj/exgen.o -c exgen.c -I/usr/include/lua50 -Irote/  -O3 -g
gcc -s -w -rdynamic -g -o bin/exgen obj/exgen.o -ldl -pthread /usr/lib/i386-linux-gnu/libsqlite3.a /usr/lib/liblua50.a /usr/lib/liblualib50.a -I/usr/include/lua50 lib/librote.a /usr/lib/i386-linux-gnu/libutil.a /usr/lib/i386-linux-gnu/libm.a /usr/lib/i386-linux-gnu/libssl.a -lssl -lcrypto -O3 -g
